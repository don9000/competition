<h1 class="jumbotron">New Competition Draw</h1>

<div class="col-md-6 col-md-offset-4">

<%= form_for @userlist do |f| %>

    <%= f.label :competition_name %>
    <%= f.text_field :name %>
    <%= f.file_field :file, class: "form-control-sm"  %> 
  <%= f.submit "Upload file", class: "btn btn-primary"%>
  <%= render 'shared/error_messages' %>

<% end %>


</div>